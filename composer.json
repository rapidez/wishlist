{
    "name": "rapidez/wishlist",
    "description": "Rapidez Wishlist",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "authors": [
        {
            "name": "Claudio Ferraro",
            "email": "claudio@justbetter.nl",
            "homepage": "https://justbetter.nl",
            "role": "Developer at JustBetter"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "rapidez/account": ">=0.13.3",
        "rapidez/core": "^2.8|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\Wishlist\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\Wishlist\\WishlistServiceProvider"
            ]
        }
    }
}
